    Name = "Mics",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
   })

--Settings

SettingsTab:AddToggle({
	Name = "Auto Haki Aura",
	Default = true,
	Callback = function(Value)
		AutoHakiAura(Value)
	end    
})

SettingsTab:AddToggle({
	Name = "Anti Afk",
	Default = true,
	Callback = function(Value)
		AntiAfk(Value)
	end    
})

--Notify Toogles

local Section = GeneralTab:AddSection({
    Name = "Select Weapon"
   })

local Section = MicsTab:AddSection({
    Name = "Change Team"
   })

   local Section = FruitTab:AddSection({
    Name = "Fruit"
   })

   local Section = ShopTab:AddSection({
    Name = "Melee Shop"
   })

   local Section = KitsuneEventTab:AddSection({
    Name = "Kitsune Event"
   })

--Button

MicsTab:AddButton({
	Name = "Pirates Team",
	Callback = function()
      		Pirates()
  	end    
})


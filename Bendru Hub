	Name = "Marines Team",
	Callback = function()
      		Marines()
  	end    
})

-- Collect Azure Ember
KitsuneEventTab:AddToggle({
	Name = "Auto Collect Azure Ember",
	Default = false,
	Callback = function(Value)
		AutoCollectAzureEmber(Value)
	end    
})
